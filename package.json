  "main": "main.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon main.js",
    "start": "cross-env NODE_ENV=production node main.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "seed:order": "npx sequelize-cli db:seed --seed src/database/seeders/20250210-seed-index.js",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
